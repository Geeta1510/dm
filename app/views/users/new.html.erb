<%= form_for @user do |f| %>
<%= f.label value="NAME  "%>
<%= f.text_field :name %><br><br>
<%= f.label value="EMAIL  "%>
<%= f.text_field :email %><br><br>
<%= f.label value="PASSWORD  "%>
<%= f.password_field :password %><br><br>
<%= f.label value="REENTER_PASSWORD  "%>
<%= f.password_field :password_confirmation %><br><br>
<%= f.submit "create Account " %>
<% end %>